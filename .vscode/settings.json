{
  "filewatcher.commands": [
    {
      "match": "\\.pug*",
      "isAsync": true,
      "cmd": "pug --pretty --out ${workspaceRoot}/online-zoo ${workspaceRoot}/src",
      "event": "onFileChange"
    },
    {
      "match": "\\.scss*",
      "isAsync": true,
      "cmd": "sass --update --stop-on-error --no-source-map ${workspaceRoot}/src:${workspaceRoot}/online-zoo",
      "event": "onFileChange"
    },
  ]
}
